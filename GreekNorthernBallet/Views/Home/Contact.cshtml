</head>
<body class="dx-viewport">
  <div class="demo-container">
    <div id="map"></div>
    <div class="options">
      <div class="caption">Options</div>
      <div class="option">
        <div id="use-custom-markers"></div>
      </div>
      <div class="option">
        <div id="show-tooltips"></div>
      </div>
    </div>
  </div>
</body>
</html>


@section Scripts
  {
  <script>
    $(function () {
      var markerUrl = "https://js.devexpress.com/Demos/RealtorApp/images/map-marker.png",
        markersData = [{
          location: [40.638918, 22.940132],
          tooltip: {
            text: "GNB Center"
          }
        }, {
          location: "40.673575, 22.905409",
          tooltip: {
            text: "GNB Euosmos"
          }
        }
        ];

      $("#map").dxMap({
        provider: "bing",
        zoom: 11,
        height: 440,
        width: "100%",
        controls: true,
        markerIconSrc: markerUrl,
        markers: markersData
      }).dxMap("instance")

    });
  </script>
}